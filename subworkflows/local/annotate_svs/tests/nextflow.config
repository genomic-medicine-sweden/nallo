process {

    withName: 'UNTAR' {
        ext.prefix = { "${name}".contains('merged') ? './vep_cache/homo_sapiens_merged/' : './vep_cache/homo_sapiens/' }
    }

    withName: 'ENSEMBLVEP_SV' {
        ext.args = { [
            '--offline',
            '--vcf',
            '--compress_output bgzip'
        ].join(' ') }
    }
}
